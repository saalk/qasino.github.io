		{
			"name": "Login",
			"item": [
				{
					"name": "Get AccessToken",
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"pm.environment.set(\"timestamp\", (Date.now()/1000).round() - 10);"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Has an access_token\", () => {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData[0].access_token).to.be.a('string');",
									"    pm.globals.set(\"access_token\", jsonData[0].access_token);",
									"});",
									"",
									"pm.test(\"Set channel\", function () {",
									"    pm.globals.set(\"channel\", pm.environment.get(\"cookie-domain\"));",
									"});",
									"",
									"const cookieJar = pm.cookies.jar();",
									"const domain = pm.environment.get(\"cookie-domain\");",
									"cookieJar.set(domain, {name: \"spin\", value: \"secure_random_value\", domain: domain}, (err, cookie) => {",
									"    console.log(\"error if occured: \" + err)",
									"    console.log(\"this is the spin cookie for \" + domain + \": \" + cookie)",
									"});",
									"",
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"protocolProfileBehavior": {
						"disabledSystemHeaders": {}
					},
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Accept",
								"value": "application/json"
							},
							{
								"key": "X-Dynscan-Options",
								"value": "out-of-scope",
								"type": "default"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "[{\n                \"access_token\": {\n                    \"issuer\": \"{{host}}\",\n                    \"client_id\": \"95851a92-3a35-4e0a-aca8-9ca7a67849df\",\n                    \"executor\": {\n                        \"grant\": \"grantId\",\n                        \"level_of_assurance\": 5,\n                        \"means\": [\n                            {\n                                \"id\": \"meansId\",\n                                \"type\": \"meansType\"\n                            }\n                        ],\n                        \"person\": \"{{person_id}}\",\n                        \"profile\": \"{{profile_id}}\",\n                        \"type\": \"customer\"\n                    },\n                    \"requester\": {\n                        \"grant\": \"grantId\",\n                        \"level_of_assurance\": 5,\n                        \"means\": [\n                            {\n                                \"id\": \"meansId\",\n                                \"type\": \"meansType\"\n                            }\n                        ],\n                        \"person\": \"{{person_id}}\",\n                        \"profile\": \"{{profile_id}}\",\n                        \"type\": \"customer\"\n                    },\n                    \"not_before\": {{$timestamp}},\n                    \"time_to_live\": 3600000,\n                    \"pinning_type\": \"cookie\",\n                    \"pinning_value\": \"{{spin-value}}\",\n                    \"scopes\": [\n                        \"personal_data\"\n                    ],\n                    \"trace_enabled\": true,\n                    \"customer_service_organisation\": \"CSO_NL\"\n                },\n                \"refresh_token\": {\n                    \"time_to_live\": 3600000\n                },\n                \"token_identifier\": \"string\",\n                \"unencrypted\": true\n            }]"
						},
						"url": {
							"raw": "https://{{host}}/security-tokens/access-tokens",
							"protocol": "https",
							"host": [
								"{{host}}"
							],
							"path": [
								"security-tokens",
								"access-tokens"
							],
							"query": [
								{
									"key": "",
									"value": null,
									"disabled": true
								}
							]
						},
						"description": "retrieve access tokens"
					},
					"response": []
				},
